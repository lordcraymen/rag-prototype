{
  "name": "rag-prototype",
  "version": "1.0.0",
  "description": "A RAG prototype using PostgreSQL + pgvector with FastMCP",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lordcraymen/rag-prototype.git"
  },
  "scripts": {
    "start": "npx tsx src/mcp/server.ts",
    "dev": "npx tsx --watch src/mcp/server.ts",
    "mcp": "npx tsx src/mcp/server.ts",
    "mcp:stdio": "npx tsx src/mcp/server.ts --transport stdio",
    "mcp:http": "npx tsx src/mcp/server.ts --transport httpStream --port 3000",
    "mcp:dev": "npx tsx src/mcp/server.ts --transport httpStream --port 3000 --verbose",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:logs": "docker logs rag-postgres",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "csv:import": "npx tsx src/scripts/csv-importer.ts"
  },
  "keywords": [
    "mcp",
    "rag",
    "fastmcp",
    "postgresql",
    "pgvector",
    "local embeddings",
    "vector database",
    "xenova"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "@xenova/transformers": "^2.17.2",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^6.1.0",
    "fastmcp": "^3.16.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.20.3",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/spy": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
