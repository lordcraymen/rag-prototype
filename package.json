{
  "name": "rag-prototype",
  "version": "1.0.0",
  "description": "A RAG prototype using PostgreSQL + pgvector with FastMCP",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lordcraymen/rag-prototype.git"
  },
  "scripts": {
    "start": "node mcp/cli.js",
    "mcp": "node mcp/cli.js",
    "mcp:stdio": "node mcp/cli.js --transport stdio",
    "mcp:http": "node mcp/cli.js --transport httpStream --port 3000",
    "mcp:sse": "node mcp/cli.js --transport sse --port 3001",
    "mcp:dev": "node mcp/cli.js --transport httpStream --port 3000 --verbose",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:logs": "docker logs rag-postgres",
    "test": "node tests/run-all-tests.js",
    "test:all": "node tests/run-all-tests.js",
    "test:config": "node tests/test-config.js",
    "test:connection": "node tests/test-connection.js",
    "test:db": "node tests/test-database.js",
    "test:system": "node tests/test-system.js"
  },
  "keywords": [
    "mcp",
    "rag",
    "fastmcp",
    "postgresql",
    "pgvector",
    "local embeddings",
    "vector database",
    "xenova"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "fastmcp": "^3.16.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  }
}
