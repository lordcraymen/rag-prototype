{
  "name": "rag-prototype",
  "version": "1.0.0",
  "description": "A RAG prototype using PostgreSQL + pgvector with FastMCP",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lordcraymen/rag-prototype.git"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "npx tsx src/mcp/server.ts",
    "dev": "npx tsx --watch src/mcp/server.ts",
    "start:legacy": "node legacy/mcp/cli.js",
    "mcp": "npx tsx src/mcp/server.ts",
    "mcp:stdio": "npx tsx src/mcp/server.ts --transport stdio",
    "mcp:http": "npx tsx src/mcp/server.ts --transport httpStream --port 3000",
    "mcp:sse": "npx tsx src/mcp/server.ts --transport sse --port 3001",
    "mcp:dev": "npx tsx src/mcp/server.ts --transport httpStream --port 3000 --verbose",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:logs": "docker logs rag-postgres",
    "test": "npx tsx tests/run-all-tests.ts",
    "test:all": "npx tsx tests/run-all-tests.ts",
    "test:config": "npx tsx tests/test-config.ts",
    "test:connection": "npx tsx tests/test-connection.ts",
    "test:db": "npx tsx tests/test-database.ts",
    "test:system": "npx tsx tests/test-system.ts",
    "csv:import": "npx tsx scripts/csv-importer.ts"
  },
  "keywords": [
    "mcp",
    "rag",
    "fastmcp",
    "postgresql",
    "pgvector",
    "local embeddings",
    "vector database",
    "xenova"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "@xenova/transformers": "^2.17.2",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^6.1.0",
    "fastmcp": "^3.16.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.20.3",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5"
  }
}
